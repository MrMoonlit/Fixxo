@model IEnumerable<CollectionItemModel>

<section class="collection-twocolumns">
    <div class="column bg-gray mt-5">
        <div class="column item-collection">
            @foreach (var item in Model)
            {
                <partial name="~/Views/Partials/_CollectionItemPartial.cshtml" model="item" />

            }
        </div>
        <div class="content">
            <h1>2 FOR USD $29</h1>
            <button class="btn-white">NEW PRODUCTS</button>
        </div>
    </div>
</section>